@model HTML_Helper_Demo.Models.Student

<h2>Form Submitted Successfully</h2>

<p><strong>Name:</strong> @Model.Name</p>
<p><strong>Bio:</strong> @Model.Bio</p>
<p><strong>Password:</strong> @Model.Password</p>
<p><strong>Id (Hidden):</strong> @Model.Id</p>
<p><strong>Is Active:</strong> @(Model.IsActive ? "Yes" : "No")</p>
<p><strong>Gender:</strong> @Model.Gender</p>
<p><strong>City:</strong> @Model.City</p>
