@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextaccessor

@{
    <h3>Cookie Created Successfully</h3>
    <div class="text-left">
        <b>User Name: </b> @HttpContextaccessor.HttpContext?.Request.Cookies["Cookieuname"]
        <br/>
        <b>User Id: </b> @HttpContextaccessor.HttpContext?.Request.Cookies["Cookieuid"]
    </div>
}
 