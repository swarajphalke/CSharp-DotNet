@using Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor;

 
@{
	<h3>Session Created Successfully...</h3>
	<div class="text-left">
	<b>User Name: </b> @HttpContextAccessor.HttpContext.Session.GetString("SessionUserName")
	<br />
	<b>User Id: </b> @HttpContextAccessor.HttpContext.Session.GetInt32("SessionUserId")
	</div>
}
